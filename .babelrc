{
  "presets": ["babel-preset-gatsby"],
  "plugins": [
    [
      "module-resolver",
      {
        "root": ["./src"],
        "alias": {
          "@elements": "./src/elements",
          "@components": "./src/components",
          "@modules": "./src/modules",
          "@images": "./src/images",
          "@styles": "./src/css",
          "@customTypes": "./src/custom-types",
          "@fakeData": "./src/fake-data",
          "@dummyImages": "./src/fake-data/images"
        }
      }
    ]
  ]
}
